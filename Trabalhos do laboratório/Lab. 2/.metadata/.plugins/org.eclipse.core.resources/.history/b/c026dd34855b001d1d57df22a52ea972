import java.util.Scanner;

public class Media {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan = new Scanner(System.in);
		double nota1;
		double total;
		
		for(int i = 0; i < 3; i++) {	
			nota1 = scan.nextDouble();
			total = nota1 + total;
		}
		System.out.printf("%f", total);

	}

}
