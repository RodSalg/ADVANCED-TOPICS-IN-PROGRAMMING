import java.util.Scanner;

public class Media {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan = new Scanner(System.in);
		double nota1 = 0.0;
		double total = 0.0;
		
		for(int i = 0; i < 3; i++) {	
			nota1 = scan.nextDouble();
			total = nota1 + total;
		}
		total = total/3;
		System.out.printf("%f", total);

	}

}
