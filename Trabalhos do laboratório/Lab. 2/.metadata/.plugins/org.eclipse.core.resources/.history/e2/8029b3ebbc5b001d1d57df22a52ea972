import java.util.Scanner;

public class MediaColecoes {
	

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int acumulador = 0;
		
		int number = 0;
		int cont = 0;
		int sum = 0;
		Scanner scan = new Scanner(System.in);
		
		
		while(number != -1) {
			
			while(number != -1) {
				sum = sum + number;
				cont = cont + 1;
				number = scan.nextInt();
			}
			System.out.printf("%d", sum/cont);
			sum = 0;
			cont = 0;
			number = scan.nextInt();
		}
		
	
	}
	
}
